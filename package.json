{
  "name": "trade-mf",
  "version": "1.0.0",
  "config": {
    "s3BucketName": "eforex.com-web",
    "region": "ap-southeast-2",
    "cloudFormationStackName": "eforex.com",
    "functionName": ""
  },
  "scripts": {
    "ng": "ng",
    "check-env": "node -e 'console.log(process.env)'",
    "start": "ng serve --port=3000 --open",
    "start:IN": "ng serve --configuration=hi-IN --port=3001 --open",
    "extract-i18n": "ng extract-i18n --output-path src/i18n",
    "build": "ng build --configuration production",
    "                 Analize": "",
    "webpack-bundle-analyzer": "ng build --configuration production --stats-json",
    "webpack-bundle-analyzer:console": "webpack-bundle-analyzer dist/TradeMF/browser/stats.json",
    "                 SSR": "",
    "dev:ssr": "ng run TradeMF:serve-ssr",
    "dev:ssr:start": "node dist/TradeMF/server/en-US/main.js",
    "build:ssr": "ng build --configuration production && ng run TradeMF:server:production",
    "prerender": "ng run TradeMF:prerender",
    "build:ssr:proxy": "node proxy-server.js",
    "node_win": "node server/server.js",
    "                 AWS": "",
    "S3:CDN:deploy eforex.com-web": "aws s3 sync ./dist/TradeMF/browser/ s3://eforex.com-web/ --profile WORKAWS",
    "                 DOCKER": "",
    "Docker_build": "docker build -t bmfn:15 .",
    "Docker_RUN": "docker run -d -p 80:80 bmfn:15",
    "Docker_RENAME": "docker tag bmfn:15 530460237251.dkr.ecr.eu-central-1.amazonaws.com/bmfn:15",
    "Docker_PUSH": "docker push 530460237251.dkr.ecr.eu-central-1.amazonaws.com/bmfn:15",
    "Docker_LOGIN": "aws ecr get-login-password --region eu-central-1 | docker login --username AWS --password-stdin 530460237251.dkr.ecr.eu-central-1.amazonaws.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.4",
    "@angular/localize": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/platform-server": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@angular/service-worker": "^14.0.4",
    "@nguniversal/express-engine": "^14.0.2",
    "express": "^4.18.1",
    "ngx-cookie-service": "^14.0.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@nguniversal/builders": "^14.0.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.0.1",
    "codelyzer": "^6.0.2",
    "dotenv": "^16.0.1",
    "jasmine-core": "^4.2.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ts-node": "^10.8.2",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "browser": {
    "crypto": false
  }
}
